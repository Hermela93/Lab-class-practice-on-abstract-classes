interface collection{
    double averageSalary();
    int calculateSth();
}
interface anotherCollection extends collection{
    double returnsomevalue();
}
abstract class Ethiopia{
    int population;
    String location;
    void averageLifeExpectancy(){

    }
    abstract double averagesalary();
}
class East_Africa extends Ethiopia{
  double averagesalary(){
   return 0;
  }
}
class World extends East_Africa{

}
abstract class Africa implements collection{
    double averageSalary(int...income){
        double sum=0;
        int i;
        double average=0;
        for(i=0;i<income.length;i++)
    {
        sum+=income[i];
    }
       average=sum/income.length;
        return average;
}
    int calculatesth(){
        int a=8;
        int b=9;
        return a+b;
    }
}
